{
  "name": "ctlorem",
  "displayName": "Chinese Lorem",
  "description": "Traditional Chinese Lorem",
  "version": "0.1.2",
  "publisher": "kevinyang",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onLanguage:html"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Chinese Lorem",
      "type": "object",
      "properties": {
        "ctlorem.includeLanguages": {
          "type": "string",
          "default": null,
          "markdownDescription": "%ctloremIncludeLanguages%"
        }
      }
    }
  },
  "homepage": "https://github.com/chgc/ctlorem",
  "repository": {
    "type": "git",
    "url": "https://github.com/chgc/ctlorem"
  },
  "bugs": {
    "url": "https://github.com/chgc/ctlorem/issues"
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "icon": "images/ctlorem-icon.png"
}
